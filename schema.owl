        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#Position"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#SymbolicDuration"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#SymbolicEvent"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#Voice"/>
        <rdfs:comment xml:lang="en">Defines the pitch modifiers of the note, in particular: demisharp , doublesharp , sharp , flat , demiflat , sesquiflat , sesquisharp , doubleflat.</rdfs:comment>
        <rdfs:label xml:lang="en">Accidental</rdfs:label>
    </owl:Class>
    


