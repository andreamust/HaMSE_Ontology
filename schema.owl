        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#ChordProgression"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#Dissonance"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#EmotionType"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#IntervalPattern"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#IntervalPatternConstituent"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#MelodicInterval"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#MelodicPattern"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#MelodicPatternConstituent"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#RhythmicPattern"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#RhythmicPatternConstituent"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#Segment"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#Structure"/>
        <rdfs:comment xml:lang="en">Any harmonic set of pitches/frequencies consisting of multiple notes (also called &quot;pitches&quot;) that are heard as if sounding simultaneously.</rdfs:comment>
        <rdfs:label xml:lang="en">Chord</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/andreapoltronieri/HaMSEontology#ChordProgression -->

    <owl:Class rdf:about="https://purl.org/andreapoltronieri/HaMSEontology#ChordProgression">
