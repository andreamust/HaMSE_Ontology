        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#F"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#G"/>
        <owl:disjointWith rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#Rest"/>
        <rdfs:comment xml:lang="en">Note A</rdfs:comment>
        <rdfs:label xml:lang="en">A</rdfs:label>
        <rdfs:label xml:lang="it">La</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/andreapoltronieri/HaMSEontology#Accidental -->

    <owl:Class rdf:about="https://purl.org/andreapoltronieri/HaMSEontology#Accidental">
        <rdfs:subClassOf rdf:resource="https://purl.org/andreapoltronieri/HaMSEontology#SymbolicRepresentation"/>
