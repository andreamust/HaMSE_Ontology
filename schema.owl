        <rdfs:label xml:lang="it">Si</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/andreapoltronieri/HaMSEontology#C -->

    <owl:Class rdf:about="https://purl.org/andreapoltronieri/HaMSEontology#C">
