    <!-- https://github.com/andreamust/HaMSE_Ontology/schema#arousesEmotionType -->

    <owl:ObjectProperty rdf:about="https://github.com/andreamust/HaMSE_Ontology/schema#arousesEmotionType">
        <owl:inverseOf rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#isArousedBy"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#MusicologicalFeature"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/MusicalWork"/>
                    <rdf:Description rdf:about="https://github.com/andreamust/HaMSE_Ontology/schema#Segment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#EmotionType"/>
        <rdfs:comment xml:lang="en">Associates the Musical Work with the Emotion Type that the piece arouses.</rdfs:comment>
        <rdfs:label xml:lang="en">arouses emotion</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/andreamust/HaMSE_Ontology/schema#belongsToChord -->

    <owl:ObjectProperty rdf:about="https://github.com/andreamust/HaMSE_Ontology/schema#belongsToChord">
        <owl:inverseOf rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#hasChordNote"/>
        <rdfs:domain rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#SymbolicEvent"/>
        <rdfs:range rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#Chord"/>
        <rdfs:comment xml:lang="en">Associates the Symbolic Event to the Chord. It is the inverse property of hasChordNote.</rdfs:comment>
        <rdfs:label xml:lang="en">belongs to chord</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://github.com/andreamust/HaMSE_Ontology/schema#belongsToIntervalPattern -->

