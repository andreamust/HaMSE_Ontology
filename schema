    <!-- https://github.com/andreamust/HaMSE_Ontology/schema#constitutesIntervalPattern -->

    <owl:ObjectProperty rdf:about="https://github.com/andreamust/HaMSE_Ontology/schema#constitutesIntervalPattern">
        <owl:inverseOf rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#hasIntervalPatternConstituent"/>
        <rdfs:domain rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#IntervalPatternConstituent"/>
