        <owl:disjointWith rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#Structure"/>
        <rdfs:comment xml:lang="en">A succession of chords. Chord progressions are the defining feature on which melody and rhythm are built (e.g. IviiiV).</rdfs:comment>
        <rdfs:label xml:lang="en">Chords Progression</rdfs:label>
    </owl:Class>
    


    <!-- https://github.com/andreamust/HaMSE_Ontology/schema#ChordVoicing -->

    <owl:Class rdf:about="https://github.com/andreamust/HaMSE_Ontology/schema#ChordVoicing">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://github.com/andreamust/HaMSE_Ontology/schema#Chord"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#constitutesProgression"/>
                        <owl:someValuesFrom rdf:resource="https://github.com/andreamust/HaMSE_Ontology/schema#ChordProgression"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
